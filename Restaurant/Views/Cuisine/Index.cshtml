
@using Restaurant.Models;
@model List<Restaurant.Models.Cuisine>;
@if(@Model.Count == 0)
{
    <p>@Html.ActionLink("Add new Cuisine", "Create")</p>
}
@foreach(Cuisine cuisineV in Model)
{
    <p>@Html.ActionLink($"{cuisineV.Name}", "Details", new {id = cuisineV.CuisineId}) </p>
}

